<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>


<class table="suspectedcriminals" name="crimeManagementSystem.SuspectedCriminals">


<id name="id" >

<generator class="increment"></generator>
</id>
<property name="image"></property>
<property name="fullname"></property>
<property name="idproof"></property>
<property name="gender"></property>
<property name="dateofbirth"></property>
<property name="address"></property>
<property name="height"></property>
<property name="weight"></property>
<property name="policestationname"></property>
<property name="officerincharge"></property>
<property name="arrestdate"></property>
<property name="remark"></property>



</class> 
</hibernate-mapping>