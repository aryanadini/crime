<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

<class table="postmortum" name="crimeManagementSystem.PostMortum">


<id name="id" >

<generator class="increment"></generator>
</id>
<property name="fullname"></property>
<property name="image"></property>
<property name="gender"></property>
<property name="dateofbirth"></property>
<property name="idproof"></property>
<property name="address"></property>
<property name="postmortum"></property>
<property name="dateofpostmortum"></property>
<property name="doctorsname"></property>
<property name="hospitaladdress"></property>
<property name="descriptionofcase"></property>


</class> 



</hibernate-mapping>